{
    "version": 2,
    "builds": [
      {
        "src": "movieserver.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "movieserver.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }